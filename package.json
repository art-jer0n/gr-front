{
  "main": "src/index.tsx",
  "name": "gr-front",
  "config": {
    "dev": "--config config/webpack.dev.js",
    "prod": "--config config/webpack.prod.js"
  },
  "version": "1.0.0",
  "scripts": {
    "webpack-config-dev": "nodemon --watch \"./config/*\" --exec npm run start-dev",
    "webpack-config-prod": "nodemon --watch \"./config/*\" --exec npm run start-prod",
    "dev": "cross-env-shell webpack serve ${npm_package_config_dev}",
    "prod": "cross-env-shell webpack serve ${npm_package_config_prod}",
    "build-dev": "cross-env-shell webpack ${npm_package_config_dev}",
    "build-prod": "cross-env-shell webpack ${npm_package_config_prod} --stats-children",
    "clean": "rd /s /q build",
    "lint": "eslint src --ext .js --ext .ts",
    "lint-fix": "eslint src --ext .js --ext .ts --fix"
  },
